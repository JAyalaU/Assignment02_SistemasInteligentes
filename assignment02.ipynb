26	"class IslandDay(Environment):\n"
27	"    \n"
28	"    #Flag to be activated when there are no more treasures or no treasures that can be gathered\n"
29	"    #agent_no_goals = False\n"
30	"    \n"
31	"    #As the environment if fully observable, the percept should contain everything found in the environment\n"
32	"    def percept(self, agent):\n"
33	"        in_existence = self.things\n"
34	"        in_posession = agent.holding\n"
35	"        at_position = self.list_things_at(agent.location)\n"
36	"        perception = [in_existence, in_posession, at_position]\n"
37	"        return perception\n"
38	"    \n"
39	"    \n"
40	"    \n"
41	"    def execute_action(self, agent, action):\n"
42	"\n"
43	"        if action == \"Move\":\n"
44	"            location = agent.location\n"
45	"            possMoves = [location]\n"
46	"            \n"
47	"            moveUp = (location[0],location[1]+1)\n"
48	"            moveDown = (location[0],location[1]-1)\n"
49	"            moveRight = (location[0]+1,location[1])\n"
50	"            moveLeft = (location[0]-1,location[1])\n"
51	"                \n"
52	"            if (moveUp[1] < 6 and len(self.list_things_at(moveUp, tclass=w)) == 0):\n"
53	"                possMoves.append(moveUp)\n"
54	"            if (moveDown[1] >= 0 and len(self.list_things_at(moveDown, tclass=w)) == 0):\n"
55	"                possMoves.append(moveDown)\n"
56	"            if (moveRight[0] < 6 and len(self.list_things_at(moveRight, tclass=w)) == 0):\n"
57	"                possMoves.append(moveRight)\n"
58	"            if (moveLeft[0] >= 0 and len(self.list_things_at(moveLeft, tclass=w)) == 0):\n"
59	"                possMoves.append(moveLeft)\n"
60	"                \n"
61	"            direction = possMoves.index(random.choice(possMoves))\n"
62	"            agent.move(possMoves[direction])  \n"
63	"            \n"
64	"        # If the action is to pick up reusable tool, run Gresure and remove the thing from the environment\n"
65	"        # Appending the object to the agent is done at the agent class. Same for all the following methods\n"
66	"        \n"
67	"        elif action == \"Greuse\":\n"
68	"            items = self.list_things_at(agent.location, tclass = H)\n"
69	"            holding = agent.get_held_things(tclass = H)\n"
70	"            if (len(items) != 0 and len(holding) == 0):\n"
71	"                agent.Greuse(items[0])\n"
72	"                self.delete_thing(items[0])\n"
73	"                \n"
74	"        elif action == \"Gdispos\":\n"
75	"            items = self.list_things_at(agent.location, tclass = h)\n"
76	"            holding = agent.get_held_things(tclass = h)\n"
77	"            if (len(items) != 0 and len(holding) == 0):\n"
78	"                agent.Gdispos(items[0])\n"
79	"                self.delete_thing(items[0])\n"
80	"                \n"
81	"        elif action == \"GTreas1\":\n"
82	"            items = self.list_things_at(agent.location, tclass = T)\n"
83	"            holding = agent.get_held_things(tclass = H)\n"
84	"            if (len(items) != 0 and len(holding) != 0):\n"
85	"                agent.GTreas1(items[0])\n"
86	"                self.delete_thing(items[0])\n"
87	"        elif action == \"GTreas2\":\n"
88	"            items = self.list_things_at(agent.location, tclass = t)\n"
89	"            if agent.holding[0] == h:\n"
90	"                agent.GTreas2(items[0])\n"
91	"                self.delete_thing(items[0])\n"
92	"        else:\n"
93	"            print('Not moving')\n"
94	"                  \n"
95	"    def is_done(self):\n"
96	"        dead_agents = not any(agent.is_alive() for agent in self.agents)\n"
97	"        return dead_agents \n"
98	"    #agent_no_goals\n"
99	"\n"
100	"    def step(self):\n"
101	"        if not self.is_done():\n"
102	"#             print(self.agents[0].holding)\n"
103	"            actions = []\n"
104	"            for agent in self.agents:\n"
105	"                if agent.alive:\n"
106	"                    actions.append(agent.program(self.percept(agent)))\n"
107	"                else:\n"
108	"                    actions.append(\"\")\n"
109	"            for (agent, action) in zip(self.agents, actions):\n"
110	"                self.execute_action(agent, action)\n"
111	"            self.exogenous_change()\n"
112	"\n"
113	"    def run(self, steps=1000):\n"
114	"        \"Run the Environment for given number of time steps.\"\n"
115	"        for step in range(steps):\n"
116	"            if self.is_done():\n"
117	"                return\n"
118	"            self.step()\n"
119	"                  \n"
120	"                  \n"
121	"       "
