1	"class Hunter(Agent):\n"
2	"    \n"
3	"    Agent.performance = 50\n"
4	"    \n"
5	"    def move(self, direc):\n"
6	"        self.performance -= 1\n"
7	"        prevLoc = self.location\n"
8	"        self.location = (direc[0],direc[1])\n"
9	"        print('Hunter: Moved from {} to {}'.format(prevLoc,self.location))\n"
10	"        \n"
11	"    def get_held_things(self, tclass=Thing):\n"
12	"        #Returns the thing held by the agent\n"
13	"        return [thing for thing in self.holding\n"
14	"                if isinstance(thing, tclass)]\n"
15	"    \n"
16	"    #If Greuse is called, the thing is appended to self.holding\n"
17	"    def Greuse(self, thing):\n"
18	"        if isinstance(thing, H):\n"
19	"            print(\"Hunter: Grabbed reusable tool at {}.\".format(self.location))\n"
20	"            self.holding.append(thing)\n"
21	"            return True\n"
22	"        return False\n"
23	"    \n"
24	"    def Gdispos(self, thing):\n"
25	"#         print(isinstance(thing,h))\n"
26	"        if isinstance(thing, h):\n"
27	"            print(\"Hunter: Grabbed disposable tool at {}.\".format(self.location))\n"
28	"            self.holding.append(thing)\n"
29	"            return True\n"
30	"        return False\n"
31	"    \n"
32	"    def GTreas1(self, thing):\n"
33	"        \n"
34	"        if isinstance(thing, T):\n"
35	"            print(\"Hunter: Grabbed a Treasure1 at {}.\".format(self.location))\n"
36	"            self.holding.append(thing)\n"
37	"            self.performance +=20\n"
38	"            print(\"Hey! my performance is: {}\".format(self.performance))\n"
39	"            return True\n"
40	"        return False\n"
41	"    \n"
42	"    #Removes disposable tool after use\n"
43	"    def GTreas2(self, thing):\n"
44	"        if isinstance(thing, t):\n"
45	"            print(\"Hunter: Grabbed a Treasure2 at {}.\".format(self.location))\n"
46	"            self.holding.append(thing)\n"
47	"            self.performance += 40\n"
48	"            print(\"Hey! my performance is: {}\".format(self.performance))\n"
49	"            for tool in self.holding:\n"
50	"                if isinstance(tool, t):\n"
51	"                    self.holding.remove(tool)\n"
52	"                    print(\"Disposable tool has been lost\")\n"
53	"                    break\n"
54	"            return True\n"
55	"        return False"
